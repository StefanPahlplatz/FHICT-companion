<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
             xmlns:tools="http://schemas.android.com/tools"
             android:layout_width="match_parent"
             android:layout_height="match_parent"

             tools:context="s.pahlplatz.fhict_companion.fragments.ResultsFragment">

    <FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
                 xmlns:tools="http://schemas.android.com/tools"
                 android:layout_width="match_parent"
                 android:layout_height="match_parent">

        <WebView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/token_webview"/>

    </FrameLayout>

    <ExpandableListView
        android:id="@+id/results_expandableListView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:indicatorRight="?android:attr/expandableListPreferredChildIndicatorRight">

    </ExpandableListView>

</FrameLayout>
